{
  "name": "angular-cli-electron-sqlite-bootstrap",
  "productName": "Angular-cli-Electron-SQLite-Bootstrap",
  "description": "An Electron application using Angular-cli, Sqlite and Bootstrap",
  "author": {
    "name": "pamtbaau"
  },
  "version": "1.0.3",
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "build:once": "ng build --base-href ./",
    "build:watch": "ng build --base-href ./ --watch",
    "test": "ng test",
    "test:codeCoverage": "ng test --codeCoverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.pamtbaau.angular-cli-electron-sqlite-bootstrap",
    "productName": "Angular-cli-Electron-SQLite-Bootstrap",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false
    },
    "files": [
      "!**/.vscode",
      "!**/build",
      "!**/config",
      "!**/assets/data",
      "!**/src"
    ],
    "extraResources": [
      {
        "from": "src/assets/data",
        "to": "dist/assets/data",
        "filter": "database*"
      }
    ]
  },
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "karma-electron": "^6.3.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.4.0",
    "sqlite3": "^5.0.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/cli": "~8.3.4",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/sqlite3": "^3.1.6",
    "codelyzer": "^5.0.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "0.0.32",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
